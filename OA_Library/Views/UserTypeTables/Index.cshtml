@model IEnumerable<OA_Library.Models.UserTypeTable>

@{
    ViewBag.Title = "All Users Deshboard";
}

@Html.ActionLink("New User Type", "Create", null, new { @class = "btn btn-success" })
<hr />
<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">User Type List</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.UserType)
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tboot>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.UserType)
                            </td>
                            <td class="btn-group btn-group-justified">
                                @Html.ActionLink("Edit", "Edit", new { id = item.UserTypeID }, new { @class = "btn btn-primary" })
                                @Html.ActionLink("Details", "Details", new { id = item.UserTypeID }, new { @class = "btn btn-info" })
                                @*@Html.ActionLink("Delete", "Delete", new { id = item.UserTypeID }, new { @class = "btn btn-danger" })*@
                            </td>
                        </tr>
                    }

                    </tbody>
            </table>
        </div>
    </div>
</div>
